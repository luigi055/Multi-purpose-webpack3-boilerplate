{
  "name": "web-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate using webpack3",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "prod": "export NODE_ENV=production || SET \"NODE_ENV=production\" && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -p",
    "build-dev": "rm -rf ./public && webpack",
    "build-dev--watch": "webpack -d -w",
    "build": "export NODE_ENV=production || SET \"NODE_ENV=production\" && rm -rf ./public && webpack -p",
    "clear": "rm -rf ./public"
  },
  "repository": {},
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "0.28.5",
    "css-mqpacker": "6.0.1",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "image-webpack-loader": "^3.2.0",
    "node-sass": "^4.5.0",
    "pixrem": "4.0.1",
    "postcss-loader": "2.0.6",
    "postcss-vmin": "3.0.0",
    "postcss-will-change": "2.0.0",
    "resolve-url-loader": "^2.0.1",
    "sass-loader": "^6.0.2",
    "script-loader": "^0.7.0",
    "style-loader": "0.18.2",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "url-loader": "^0.5.7",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "express": "4.15.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "popper.js": "^1.12.3",
    "tether": "^1.4.0"
  }
}
